{
    "env": {
        "browser": true
    },
    "rules": {
        "no-console": 1,
        "global-strict": 2,
        "handle-callback-err": 0,
        "no-mixed-requires": 0,
        "no-new-require": 0,
        "no-path-concat": 0,
        "no-process-exit": 0,
        "no-restricted-modules": 0,
        "no-sync": 0,
    }
}
